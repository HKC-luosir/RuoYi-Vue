    <select id="selectTRoomCleanRecordH5List" parameterType="TRoomCleanRecord" resultType="CleanRecordH5Vo">
        select c.*,r.name as roomName, s.name as storeName
        from t_room_clean_record c
            left join t_room r on c.room_id =r.id
            left join t_store s on r.store_id =s.id
        <where>
