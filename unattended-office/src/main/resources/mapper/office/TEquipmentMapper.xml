<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
"http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.ruoyi.office.mapper.TEquipmentMapper">
    
    <resultMap type="TEquipment" id="TEquipmentResult">
        <result property="id"    column="id"    />
        <result property="name"    column="name"    />
        <result property="equipType"    column="equip_type"    />
        <result property="online"    column="online"    />
        <result property="onOff"    column="on_off"    />
        <result property="equipControl"    column="equip_control"    />
        <result property="recentOpenTime"    column="recent_open_time"    />
        <result property="status"    column="status"    />
        <result property="remark"    column="remark"    />
        <result property="createBy"    column="create_by"    />
        <result property="createTime"    column="create_time"    />
        <result property="updateBy"    column="update_by"    />
        <result property="updateTime"    column="update_time"    />
    </resultMap>

    <sql id="selectTEquipmentVo">
        select id, name, equip_type, online, on_off, equip_control, recent_open_time, status, remark, create_by, create_time, update_by, update_time from t_equipment
    </sql>

    <select id="selectTEquipmentList" parameterType="TEquipment" resultMap="TEquipmentResult">
        <include refid="selectTEquipmentVo"/>
        <where>  
            <if test="name != null  and name != ''"> and name like concat('%', #{name}, '%')</if>
            <if test="equipType != null  and equipType != ''"> and equip_type = #{equipType}</if>
            <if test="online != null "> and online = #{online}</if>
            <if test="onOff != null "> and on_off = #{onOff}</if>
            <if test="equipControl != null  and equipControl != ''"> and equip_control = #{equipControl}</if>
            <if test="status != null "> and status = #{status}</if>
            <if test="createBy != null  and createBy != ''"> and create_by = #{createBy}</if>
        </where>
    </select>
    
    <select id="selectTEquipmentById" parameterType="Long" resultMap="TEquipmentResult">
        <include refid="selectTEquipmentVo"/>
        where id = #{id}
    </select>
    <select id="selectAvailableEquipmentList" parameterType="TEquipment" resultMap="TEquipmentResult">

    </select>

    <insert id="insertTEquipment" parameterType="TEquipment" useGeneratedKeys="true" keyProperty="id">
        insert into t_equipment
        <trim prefix="(" suffix=")" suffixOverrides=",">
            <if test="name != null and name != ''">name,</if>
            <if test="equipType != null and equipType != ''">equip_type,</if>
            <if test="online != null">online,</if>
            <if test="onOff != null">on_off,</if>
            <if test="equipControl != null">equip_control,</if>
            <if test="recentOpenTime != null">recent_open_time,</if>
            <if test="status != null">status,</if>
            <if test="remark != null">remark,</if>
            <if test="createBy != null">create_by,</if>
            <if test="createTime != null">create_time,</if>
            <if test="updateBy != null">update_by,</if>
            <if test="updateTime != null">update_time,</if>
         </trim>
        <trim prefix="values (" suffix=")" suffixOverrides=",">
            <if test="name != null and name != ''">#{name},</if>
            <if test="equipType != null and equipType != ''">#{equipType},</if>
            <if test="online != null">#{online},</if>
            <if test="onOff != null">#{onOff},</if>
            <if test="equipControl != null">#{equipControl},</if>
            <if test="recentOpenTime != null">#{recentOpenTime},</if>
            <if test="status != null">#{status},</if>
            <if test="remark != null">#{remark},</if>
            <if test="createBy != null">#{createBy},</if>
            <if test="createTime != null">#{createTime},</if>
            <if test="updateBy != null">#{updateBy},</if>
            <if test="updateTime != null">#{updateTime},</if>
         </trim>
    </insert>

    <update id="updateTEquipment" parameterType="TEquipment">
        update t_equipment
        <trim prefix="SET" suffixOverrides=",">
            <if test="name != null and name != ''">name = #{name},</if>
            <if test="equipType != null and equipType != ''">equip_type = #{equipType},</if>
            <if test="online != null">online = #{online},</if>
            <if test="onOff != null">on_off = #{onOff},</if>
            <if test="equipControl != null">equip_control = #{equipControl},</if>
            <if test="recentOpenTime != null">recent_open_time = #{recentOpenTime},</if>
            <if test="status != null">status = #{status},</if>
            <if test="remark != null">remark = #{remark},</if>
            <if test="createBy != null">create_by = #{createBy},</if>
            <if test="createTime != null">create_time = #{createTime},</if>
            <if test="updateBy != null">update_by = #{updateBy},</if>
            <if test="updateTime != null">update_time = #{updateTime},</if>
        </trim>
        where id = #{id}
    </update>

    <delete id="deleteTEquipmentById" parameterType="Long">
        delete from t_equipment where id = #{id}
    </delete>

    <delete id="deleteTEquipmentByIds" parameterType="String">
        delete from t_equipment where id in 
        <foreach item="id" collection="array" open="(" separator="," close=")">
            #{id}
        </foreach>
    </delete>
</mapper>