<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
"http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.ruoyi.office.mapper.TMqttMsgMapper">
    
    <resultMap type="TMqttMsg" id="TMqttMsgResult">
        <result property="id"    column="id"    />
        <result property="msgType"    column="msg_type"    />
        <result property="topic"    column="topic"    />
        <result property="qos"    column="qos"    />
        <result property="message"    column="message"    />
        <result property="clientId"    column="client_id"    />
        <result property="status"    column="status"    />
        <result property="createBy"    column="create_by"    />
        <result property="createTime"    column="create_time"    />
        <result property="updateBy"    column="update_by"    />
        <result property="updateTime"    column="update_time"    />
        <result property="remark"    column="remark"    />
    </resultMap>

    <sql id="selectTMqttMsgVo">
        select id, msg_type, topic,qos, message, client_id, status, create_by, create_time, update_by, update_time, remark from t_mqtt_msg
    </sql>

    <select id="selectTMqttMsgList" parameterType="TMqttMsg" resultMap="TMqttMsgResult">
        <include refid="selectTMqttMsgVo"/>
        <where>  
            <if test="msgType != null  and msgType != ''"> and msg_type = #{msgType}</if>
            <if test="topic != null  and topic != ''"> and topic = #{topic}</if>
            <if test="qos != null  and qos != ''"> and qos = #{qos}</if>
            <if test="message != null  and message != ''"> and message = #{message}</if>
            <if test="clientId != null  and clientId != ''"> and client_id = #{clientId}</if>
            <if test="status != null  and status != ''"> and status = #{status}</if>
        </where>
    </select>
    
    <select id="selectTMqttMsgById" parameterType="Long" resultMap="TMqttMsgResult">
        <include refid="selectTMqttMsgVo"/>
        where id = #{id}
    </select>
        
    <insert id="insertTMqttMsg" parameterType="TMqttMsg" useGeneratedKeys="true" keyProperty="id">
        insert into t_mqtt_msg
        <trim prefix="(" suffix=")" suffixOverrides=",">
            <if test="msgType != null and msgType != ''">msg_type,</if>
            <if test="topic != null and topic != ''">topic,</if>
            <if test="qos != null and qos != ''">qos,</if>
            <if test="message != null and message != ''">message,</if>
            <if test="clientId != null">client_id,</if>
            <if test="status != null">status,</if>
            <if test="createBy != null">create_by,</if>
            <if test="createTime != null">create_time,</if>
            <if test="updateBy != null">update_by,</if>
            <if test="updateTime != null">update_time,</if>
            <if test="remark != null">remark,</if>
         </trim>
        <trim prefix="values (" suffix=")" suffixOverrides=",">
            <if test="msgType != null and msgType != ''">#{msgType},</if>
            <if test="topic != null and topic != ''">#{topic},</if>
            <if test="qos != null and qos != ''">#{qos},</if>
            <if test="message != null and message != ''">#{message},</if>
            <if test="clientId != null">#{clientId},</if>
            <if test="status != null">#{status},</if>
            <if test="createBy != null">#{createBy},</if>
            <if test="createTime != null">#{createTime},</if>
            <if test="updateBy != null">#{updateBy},</if>
            <if test="updateTime != null">#{updateTime},</if>
            <if test="remark != null">#{remark},</if>
         </trim>
    </insert>

    <update id="updateTMqttMsg" parameterType="TMqttMsg">
        update t_mqtt_msg
        <trim prefix="SET" suffixOverrides=",">
            <if test="msgType != null and msgType != ''">msg_type = #{msgType},</if>
            <if test="topic != null and topic != ''">topic = #{topic},</if>
            <if test="qos != null and qos != ''">qos = #{qos},</if>
            <if test="message != null and message != ''">message = #{message},</if>
            <if test="clientId != null">client_id = #{clientId},</if>
            <if test="status != null">status = #{status},</if>
            <if test="createBy != null">create_by = #{createBy},</if>
            <if test="createTime != null">create_time = #{createTime},</if>
            <if test="updateBy != null">update_by = #{updateBy},</if>
            <if test="updateTime != null">update_time = #{updateTime},</if>
            <if test="remark != null">remark = #{remark},</if>
        </trim>
        where id = #{id}
    </update>

    <delete id="deleteTMqttMsgById" parameterType="Long">
        delete from t_mqtt_msg where id = #{id}
    </delete>

    <delete id="deleteTMqttMsgByIds" parameterType="String">
        delete from t_mqtt_msg where id in 
        <foreach item="id" collection="array" open="(" separator="," close=")">
            #{id}
        </foreach>
    </delete>
</mapper>