    <select id="selectTRoomOrderCancelList" parameterType="TRoomOrderCancel" resultMap="TRoomOrderCancelResult">
        <include refid="selectTRoomOrderCancelVo"/>
        <where>  
            <if test="cancelMinutes != null "> and cancel_minutes = #{cancelMinutes}</if>
            <if test="refundRate != null "> and refund_rate = #{refundRate}</if>
            <if test="status != null "> and status = #{status}</if>
        </where>
    </select>
    
    <select id="selectTRoomOrderCancelById" parameterType="Long" resultMap="TRoomOrderCancelResult">
        <include refid="selectTRoomOrderCancelVo"/>
        where id = #{id}
    </select>
        
    <insert id="insertTRoomOrderCancel" parameterType="TRoomOrderCancel" useGeneratedKeys="true" keyProperty="id">
        insert into t_room_order_cancel
        <trim prefix="(" suffix=")" suffixOverrides=",">
            <if test="cancelMinutes != null">cancel_minutes,</if>
            <if test="refundRate != null">refund_rate,</if>
            <if test="status != null">status,</if>
