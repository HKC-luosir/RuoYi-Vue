    <delete id="deleteTRoomOrderCancelById" parameterType="Long">
        delete from t_room_order_cancel where id = #{id}
    </delete>

    <delete id="deleteTRoomOrderCancelByIds" parameterType="String">
        delete from t_room_order_cancel where id in 
        <foreach item="id" collection="array" open="(" separator="," close=")">
            #{id}
        </foreach>
