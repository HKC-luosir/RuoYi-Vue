page{
  background-color: #f8f8f9;
}
.staffItem{
  padding: 10px 0;
  border-radius: 10px;
  background-color: #ffffff;
}
.staffAvatarView{
  width: 100px;
  height: 100px;
  position: relative;
}
.avatarImg{
  width: 80px;
  height: 80px;
  border-radius: 80px;
}
.avatarFrame{
  width: 120px;
  position: absolute;
  top: -20px;
  left: -20px;
  z-index: 1;
}
.staffVoice{
  height: 16px;
  overflow: hidden;
  padding: 3px 10px;
  position: absolute;
  bottom: 0;
  left: -10px;
  z-index: 5;
  border-radius: 10px;
}
.voicePlay{
  width: 12px;
  margin-right: 4px;
}
.voiceAudioImg{
  width: 50px;
}
.audioFrequencyGif{
  width: 50px;
}
.voiceSeconds{
  color: #ffffff;
  font-size: 10px;
  margin-left: 4px;
}
.staffItem .staffName{
  font-size: 14px;
  font-weight: bold;
  margin-right: 10px;
}
.staffBadgeLevel{
  font-size: 10px;
  font-weight: bold;
  color: #ffffff;
  padding: 2px 10px;
  border-radius: 10px;
}
.staffBadgeLevel image{
  width: 14px;
  margin-right: 4px;
}
.staffOnLineState{
  font-size: 12px;
}
.staffOnLineState image{
  width: 12px;
  height: 12px;
  margin-right: 4px;
}
.staffBaseInfo{
  font-size: 12px;
  margin-top: 2px;
  color: #80848f;
}
.staffBaseInfo view{
  margin-right: 6px;
}
.staffDetails{
  font-size: 12px;
  margin-top: 4px;
}