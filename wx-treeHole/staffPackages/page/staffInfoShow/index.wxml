<view class="bView">

  <!-- 头部状态栏 -->
  <view class="stateBarView">
    <view style="height:{{stateBarHeight}}px;background:{{barThemeColor}};" class="stateBar"></view>
    <view style="height:{{barHeight}}px;background:{{barThemeColor}};" class="stateBar">
      <view class="view_box_common titleView" style="height: 100%;">
        <view class="stateBarIcoView" bind:tap="toBack">
          <wux-icon type="ios-arrow-back" size="24" color="#ffffff"></wux-icon>
        </view>
        <text>店员资料卡</text>
      </view>
    </view>
  </view>

  <scroll-view scroll-y refresher-enabled="true" refresher-background="#555d8c" refresher-default-style="white" refresher-triggered="{{refreshState}}" style="width:{{pWidth}}px;height:{{pHeight}}px;" bindscroll="scrollView">
    <view class="bgImg" style="height: {{pHeight*0.37}}px;">
      <image src="../../../static/temp/1.jpg" mode="widthFix"></image>
    </view>
    <!-- 个人信息 -->
    <view class="view_box_common scope_view" style="position: relative;top: -72px;">
      <view class="view_box_common view_box_row_content_symmetry" style="height:100px;">
        <!-- 头像 -->
        <view class="txView">
          <image src="../../../static/temp/2.jpg" class="view_all_adapt"></image>
          <view class="view_box_row_content boy_bg_color">
            <image class="staffAudioPlay" src="../../../static/ico/play.png" mode="widthFix"></image>
            <image class="staffAudioFrequency" src="../../../static/ico/audio_frequency.png" mode="widthFix"></image>
            <span>14s</span>
          </view>
        </view>
        <!-- 店员具体数据 -->
        <view class="view_box_column_symmetry" style="width: {{pWidth*0.92*0.92-110}}px;height: 92%;">
          <view>
            <view class="view_box_row_content_symmetry">
              <!-- 名字和金牌情况 -->
              <view class="view_box_row_content">
                <view class="staffName">魔爪来袭</view>
                <view class="staffBaseInfo">
                  广东-清远
                </view>
                <view class="staffBadgeLevel view_box_row_content badgeLevel_4">
                  <image src="../../../static/images/badge_level_4.png" mode="widthFix"></image>
                  <view>女神</view>
                </view>
              </view>
              <view class="view_box_row_content onLineState">
                <image src="../../../static/ico/online_32.png"></image>
                <view>在线</view>
              </view>
            </view>
            <!-- 员工服务 -->
            <view class="view_box_row">
              <view class="staffService">
                <span>文语</span><text>|</text><span>语聊</span><text>|</text><span>视频</span><text>|</text><span>游戏</span><text>|</text><span>虚恋</span><text>|</text><span>哄睡</span><text>|</text><span>歌单</span>
              </view>
            </view>
          </view>
          <!-- 员工标签 -->
          <view class="staffLabel view_box_row_wrap">
            <view class="girl_edging_color girl_font_color staffLabelItem">27岁👧</view>
            <view class="girl_edging_color girl_font_color staffLabelItem">淘气人妻人妻🎈</view>
            <view class="girl_edging_color girl_font_color staffLabelItem">恶毒前妻🍑</view>
            <view class="girl_edging_color girl_font_color staffLabelItem">心动在此心动在此</view>
            <view class="girl_edging_color girl_font_color staffLabelItem">淘气人妻🎈</view>
            <view class="girl_edging_color girl_font_color staffLabelItem">恶毒前妻🍑</view>
          </view>
        </view>
      </view>
      <view class="seizeSeat_mini"></view>
      <view class="view_box_common">
        <view class="cut_off_rule_small"></view>
      </view>
      <view class="seizeSeat_mini"></view>
