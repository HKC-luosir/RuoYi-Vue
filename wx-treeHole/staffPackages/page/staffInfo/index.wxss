page{
  background-color: #f8f8f9;
}
.scope_view{
  background-color: #ffffff;
  padding: 10px 0;
  border-radius: 10px;
}
.tempColumn{
  font-size: 16px;
  font-weight: bold;
}
.tempColumn image{
  width: 12px;
  height: 12px;
  margin-right: 4px;
}
.serviceItem{
  font-size: 14px;
  padding-top: 10px;
}
.serviceItem span{
  margin-right: 4px;
}
.inputItem{
  text-align: right;
  font-size: 14px;
}
.imitateWuxCell{
  padding: 20rpx 30rpx;
