  color: #ffffff;
  padding: 2px 10px;
  border-radius: 10px;
  font-size: 12px;
}
.labelAddBut view{
  margin-left: 4px;
}
.labelItem{
  margin-top: 10px;
  margin-right: 10px;
  padding: 2px 10px;
  border-radius: 10px;
  position: relative;
  overflow: hidden;
  padding-right: 16px;
}
.labelItem image{
  width: 16px;
  position: absolute;
  top: 0;
  right: 0;
}
.userTx{
  width: 56px;
  height: 56px;
  border-radius: 56px;
  overflow: hidden;
  box-shadow: 0 0 5px 2px #8978E4;
}
.switchSoundRecordView{
  font-size: 14px;
}
.switchSoundRecordView image{
  width: 14px;
  margin-right: 4px;
}
.switchSoundRecordView span{
  line-height: 16px;
}
