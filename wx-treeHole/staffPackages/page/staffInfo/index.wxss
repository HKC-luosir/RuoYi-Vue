  font-size: 14px;
  margin-left: 10px;
  color: #ffffff;
  padding: 4px 10px;
  border-radius: 10px;
}
.soundRecordBut image{
  width: 16px;
  margin-right: 4px;
}
.audioBut{
  height: 20px;
  overflow: hidden;
  padding: 4px 10px;
  border-radius: 10px;
  color: #ffffff;
  font-size: 14px;
}
.audioPlay{
  width: 16px;
  margin-right: 10px;
}
.audioFrequency{
  width: 100px;
  margin-right: 10px;
}
.audioFrequencyGif{
  width: 100px;
  margin-right: 10px;
}
.submitView{
  width: 100%;
  padding: 20px 0;
