<view class="bView">
  <!-- 头部状态栏 -->
  <view class="stateBarView">
    <view style="height:{{stateBarHeight}}px;background:{{barThemeColor}};" class="stateBar"></view>
    <view style="height:{{barHeight}}px;background:{{barThemeColor}};" class="stateBar">
      <view class="view_box_common titleView" style="height: 100%;">
        <view class="stateBarIcoView">
        </view>
        <text>听 说</text>
      </view>
    </view>
  </view>

  <!-- body -->
  <scroll-view scroll-y refresher-enabled="true" refresher-background="#555d8c" refresher-default-style="white" refresher-triggered="{{refreshState}}" style="width:{{pWidth}}px;height:{{pHeight}}px;">
    <!-- banner -->
    <view class="bannerBgColorView" style="height:{{pHeight * 0.3}}px;background-color:{{banners!=null?banners[bannerCurrent].themeColor:'#555d8c'}};"></view>
    <view style="width: 100%;height: {{showStateBarHeight+10}}px;"></view>
    <swiper class="swiperView" style="height:{{pHeight*0.23}}px;background-color:{{banners!=null?banners[bannerCurrent].themeColor:'#555d8c'}};" bindchange="swiperChange" indicator-active-color="#ffffff" indicator-dots="true" autoplay='true' duration='1000' interval='3000'>
      <block wx:for="{{banners}}" wx:key="id" wx:for-item="item">
        <swiper-item data-toLink="{{item.toLink}}">
          <image class="view_all_adapt" src="{{item.imgUrl}}" />
        </swiper-item>
      </block>
    </swiper>

    <!-- 消息滚动 -->
    <view class="noticeView view_box_common girl_bg_color">
