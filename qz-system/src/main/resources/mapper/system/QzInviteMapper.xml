<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
"http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.ruoyi.system.mapper.QzInviteMapper">
    
    <resultMap type="QzInvite" id="QzInviteResult">
        <result property="id"    column="id"    />
        <result property="qzId"    column="qz_id"    />
        <result property="exp"    column="exp"    />
        <result property="createUserId"    column="create_user_id"    />
        <result property="receiveUserId"    column="receive_user_id"    />
        <result property="status"    column="status"    />
        <result property="createBy"    column="create_by"    />
        <result property="createTime"    column="create_time"    />
        <result property="updateBy"    column="update_by"    />
        <result property="updateTime"    column="update_time"    />
        <result property="remark"    column="remark"    />
        <result property="ext1"    column="ext1"    />
        <result property="ext2"    column="ext2"    />
        <result property="ext3"    column="ext3"    />
    </resultMap>

    <sql id="selectQzInviteVo">
        select id, qz_id, exp, create_user_id, receive_user_id, status, create_by, create_time, update_by, update_time, remark, ext1, ext2, ext3 from qz_invite
    </sql>

    <select id="selectQzInviteList" parameterType="QzInvite" resultMap="QzInviteResult">
        <include refid="selectQzInviteVo"/>
        <where>  
            <if test="qzId != null "> and qz_id = #{qzId}</if>
            <if test="exp != null "> and exp = #{exp}</if>
            <if test="createUserId != null "> and create_user_id = #{createUserId}</if>
            <if test="receiveUserId != null "> and receive_user_id = #{receiveUserId}</if>
            <if test="status != null  and status != ''"> and status = #{status}</if>
        </where>
    </select>
    
    <select id="selectQzInviteById" parameterType="Long" resultMap="QzInviteResult">
        <include refid="selectQzInviteVo"/>
        where id = #{id}
    </select>
        
    <insert id="insertQzInvite" parameterType="QzInvite" useGeneratedKeys="true" keyProperty="id">
        insert into qz_invite
        <trim prefix="(" suffix=")" suffixOverrides=",">
            <if test="qzId != null">qz_id,</if>
            <if test="exp != null">exp,</if>
            <if test="createUserId != null">create_user_id,</if>
            <if test="receiveUserId != null">receive_user_id,</if>
            <if test="status != null">status,</if>
            <if test="createBy != null">create_by,</if>
            <if test="createTime != null">create_time,</if>
            <if test="updateBy != null">update_by,</if>
            <if test="updateTime != null">update_time,</if>
            <if test="remark != null">remark,</if>
            <if test="ext1 != null">ext1,</if>
            <if test="ext2 != null">ext2,</if>
            <if test="ext3 != null">ext3,</if>
         </trim>
        <trim prefix="values (" suffix=")" suffixOverrides=",">
            <if test="qzId != null">#{qzId},</if>
            <if test="exp != null">#{exp},</if>
            <if test="createUserId != null">#{createUserId},</if>
            <if test="receiveUserId != null">#{receiveUserId},</if>
            <if test="status != null">#{status},</if>
            <if test="createBy != null">#{createBy},</if>
            <if test="createTime != null">#{createTime},</if>
            <if test="updateBy != null">#{updateBy},</if>
            <if test="updateTime != null">#{updateTime},</if>
            <if test="remark != null">#{remark},</if>
            <if test="ext1 != null">#{ext1},</if>
            <if test="ext2 != null">#{ext2},</if>
            <if test="ext3 != null">#{ext3},</if>
         </trim>
    </insert>

    <update id="updateQzInvite" parameterType="QzInvite">
        update qz_invite
        <trim prefix="SET" suffixOverrides=",">
            <if test="qzId != null">qz_id = #{qzId},</if>
            <if test="exp != null">exp = #{exp},</if>
            <if test="createUserId != null">create_user_id = #{createUserId},</if>
            <if test="receiveUserId != null">receive_user_id = #{receiveUserId},</if>
            <if test="status != null">status = #{status},</if>
            <if test="createBy != null">create_by = #{createBy},</if>
            <if test="createTime != null">create_time = #{createTime},</if>
            <if test="updateBy != null">update_by = #{updateBy},</if>
            <if test="updateTime != null">update_time = #{updateTime},</if>
            <if test="remark != null">remark = #{remark},</if>
            <if test="ext1 != null">ext1 = #{ext1},</if>
            <if test="ext2 != null">ext2 = #{ext2},</if>
            <if test="ext3 != null">ext3 = #{ext3},</if>
        </trim>
        where id = #{id}
    </update>

    <delete id="deleteQzInviteById" parameterType="Long">
        delete from qz_invite where id = #{id}
    </delete>

    <delete id="deleteQzInviteByIds" parameterType="String">
        delete from qz_invite where id in 
        <foreach item="id" collection="array" open="(" separator="," close=")">
            #{id}
        </foreach>
    </delete>
</mapper>