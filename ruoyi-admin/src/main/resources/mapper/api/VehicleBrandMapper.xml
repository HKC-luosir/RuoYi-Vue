<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
"http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.ruoyi.system.mapper.VehicleBrandMapper">
    
    <resultMap type="VehicleBrand" id="VehicleBrandResult">
        <result property="id"    column="id"    />
        <result property="brand"    column="brand"    />
        <result property="notes"    column="notes"    />
        <result property="url"    column="url"    />
        <result property="createTime"    column="create_time"    />
        <result property="updateTime"    column="update_time"    />
    </resultMap>

    <sql id="selectVehicleBrandVo">
        select id, brand, notes, url, create_time, update_time from vehicle_brand
    </sql>

    <select id="selectVehicleBrandList" parameterType="VehicleBrand" resultMap="VehicleBrandResult">
        <include refid="selectVehicleBrandVo"/>
        <where>  
            <if test="brand != null  and brand != ''"> and brand = #{brand}</if>
            <if test="notes != null  and notes != ''"> and notes = #{notes}</if>
            <if test="url != null  and url != ''"> and url = #{url}</if>
        </where>
    </select>
    
    <select id="selectVehicleBrandById" parameterType="Long" resultMap="VehicleBrandResult">
        <include refid="selectVehicleBrandVo"/>
        where id = #{id}
    </select>
        
    <insert id="insertVehicleBrand" parameterType="VehicleBrand" useGeneratedKeys="true" keyProperty="id">
        insert into vehicle_brand
        <trim prefix="(" suffix=")" suffixOverrides=",">
            <if test="brand != null and brand != ''">brand,</if>
            <if test="notes != null">notes,</if>
            <if test="url != null and url != ''">url,</if>
            <if test="createTime != null">create_time,</if>
            <if test="updateTime != null">update_time,</if>
         </trim>
        <trim prefix="values (" suffix=")" suffixOverrides=",">
            <if test="brand != null and brand != ''">#{brand},</if>
            <if test="notes != null">#{notes},</if>
            <if test="url != null and url != ''">#{url},</if>
            <if test="createTime != null">#{createTime},</if>
            <if test="updateTime != null">#{updateTime},</if>
         </trim>
    </insert>

    <update id="updateVehicleBrand" parameterType="VehicleBrand">
        update vehicle_brand
        <trim prefix="SET" suffixOverrides=",">
            <if test="brand != null and brand != ''">brand = #{brand},</if>
            <if test="notes != null">notes = #{notes},</if>
            <if test="url != null and url != ''">url = #{url},</if>
            <if test="createTime != null">create_time = #{createTime},</if>
            <if test="updateTime != null">update_time = #{updateTime},</if>
        </trim>
        where id = #{id}
    </update>

    <delete id="deleteVehicleBrandById" parameterType="Long">
        delete from vehicle_brand where id = #{id}
    </delete>

    <delete id="deleteVehicleBrandByIds" parameterType="String">
        delete from vehicle_brand where id in 
        <foreach item="id" collection="array" open="(" separator="," close=")">
            #{id}
        </foreach>
    </delete>
</mapper>