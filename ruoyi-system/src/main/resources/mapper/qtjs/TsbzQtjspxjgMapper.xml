    <sql id="selectTsbzQtjspxjgVo">
--         select id, faid, jsid, pxlx, nf, createuserid, create_time from tsbz_qtjspxjg
select  a.id, a.faid, a.jsid, a.pxlx, a.nf, a.createuserid, a.create_time,
b.jsid, b.jsxm, b.xb, b.deptid, b.dabh, b.zjlx, b.zjhm, b.bzzt, b.gwzt, b.gwlx, b.csrq, b.rjnx, b.zc, b.zyry, b.tzb, b.zks,
c.xxmc,
d.name as pxfamc from tsbz_qtjspxjg a
left join tsbz_jsjbxx b on a.jsid=b.id
left join tsbz_xxjbxx c on b.deptid=c.id
left join tsbz_qtjspxfa d on a.faid=d.id
    </sql>

    <select id="selectTsbzQtjspxjgList" parameterType="TsbzQtjspxjg" resultMap="TsbzQtjspxjgResult">
        <include refid="selectTsbzQtjspxjgVo"/>
        <where>
            <if test="faid != null ">and a.faid = #{faid}</if>
            <if test="jsid != null ">and a.jsid = #{jsid}</if>
            <if test="pxlx != null  and pxlx != ''">and a.pxlx = #{pxlx}</if>
            <if test="nf != null  and nf != ''">and a.nf = #{nf}</if>
        </where>
    </select>

    <select id="selectTsbzQtjspxjgById" parameterType="Long" resultMap="TsbzQtjspxjgResult">
