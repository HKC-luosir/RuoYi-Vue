    <select id="selectTsbzQtjspxjgKhjdList" parameterType="TsbzQtjspxjg" resultMap="TsbzQtjspxjgResult">
        select b.id, b.jsid, b.jsxm, b.xb, b.deptid, b.dabh, b.zjlx, b.zjhm, b.bzzt, b.gwzt, b.gwlx, b.csrq, b.rjnx, b.zc, b.zyry, b.tzb, b.zks, b.create_time,
