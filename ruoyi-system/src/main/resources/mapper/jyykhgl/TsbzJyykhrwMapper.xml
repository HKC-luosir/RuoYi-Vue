<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.ruoyi.jyykhgl.mapper.TsbzJyykhrwMapper">

    <resultMap type="TsbzJyykhrw" id="TsbzJyykhrwResult">
        <result property="id" column="id"/>
        <result property="userId" column="user_id"/>
        <result property="nickName" column="nick_name"/>
        <result property="rwmc" column="rwmc"/>
        <result property="rwlx" column="rwlx"/>
        <result property="rwnr" column="rwnr"/>
        <result property="khbm" column="khbm"/>
        <result property="khzq" column="khzq"/>
        <result property="slyq" column="slyq"/>
        <result property="fsbx" column="fsbx"/>
        <result property="isdel" column="isdel"/>
        <result property="createTime" column="create_time"/>
        <result property="rwsm" column="rwsm"/>
        <result property="deptId" column="dept_id"/>
        <result property="wcsl" column="wcsl"/>
    </resultMap>

    <sql id="selectTsbzJyykhrwVo">
        select u.id, u.rwmc, u.rwlx, u.rwnr, u.khbm, u.khzq, u.slyq, u.fsbx, u.isdel, u.create_time, u.rwsm, u.dept_id from tsbz_jyykhrw u
        left join sys_dept d on u.dept_id = d.dept_id
    </sql>

    <select id="selectTsbzJyykhrwList" parameterType="TsbzJyykhrw" resultMap="TsbzJyykhrwResult">
        <include refid="selectTsbzJyykhrwVo"/>
        where u.isdel ='N'
        <if test="rwmc != null  and rwmc != ''">and u.rwmc = #{rwmc}</if>
        <if test="rwlx != null  and rwlx != ''">and u.rwlx = #{rwlx}</if>
        <if test="rwnr != null  and rwnr != ''">and u.rwnr = #{rwnr}</if>
        <if test="khbm != null  and khbm != ''">and u.khbm = #{khbm}</if>
        <if test="khzq != null  and khzq != ''">and u.khzq = #{khzq}</if>
        <if test="slyq != null  and slyq != ''">and u.slyq = #{slyq}</if>
        <if test="fsbx != null  and fsbx != ''">and u.fsbx = #{fsbx}</if>
        <if test="rwsm != null  and rwsm != ''">and u.rwsm = #{rwsm}</if>

        <!-- 数据范围过滤 -->
        ${params.dataScope}
    </select>

    <select id="selectTsbzJyykhrwById" parameterType="Long" resultMap="TsbzJyykhrwResult">
        <include refid="selectTsbzJyykhrwVo"/>
        where id = #{id}
    </select>

    <select id="selectTsbzJyykhrwStatistics" parameterType="TsbzJyykhrw" resultMap="TsbzJyykhrwResult">
        select u.id, d.user_id, d.nick_name, u.rwmc, u.rwlx, u.rwnr, u.khbm, u.khzq, u.slyq, u.fsbx, u.isdel,
        u.create_time, u.rwsm, u.dept_id,
        concat((select count(*) from v_jyykhrwtj where rwid=u.id and create_userid=d.user_id)/slyq*100,'%') as wcsl
        from sys_user d
        left join tsbz_jyykhrw u on d.dept_id=u.dept_id
        where u.isdel ='N'
        <if test="id != null  and id != ''">and u.id = #{id}</if>
        <if test="rwmc != null  and rwmc != ''">and u.rwmc = #{rwmc}</if>
        <if test="rwlx != null  and rwlx != ''">and u.rwlx = #{rwlx}</if>
        <if test="rwnr != null  and rwnr != ''">and u.rwnr = #{rwnr}</if>
        <if test="khbm != null  and khbm != ''">and u.khbm = #{khbm}</if>
        <if test="khzq != null  and khzq != ''">and u.khzq = #{khzq}</if>
        <if test="slyq != null  and slyq != ''">and u.slyq = #{slyq}</if>
        <if test="fsbx != null  and fsbx != ''">and u.fsbx = #{fsbx}</if>
        <if test="rwsm != null  and rwsm != ''">and u.rwsm = #{rwsm}</if>

        <!-- 数据范围过滤 -->
        ${params.dataScope}
    </select>

    <insert id="insertTsbzJyykhrw" parameterType="TsbzJyykhrw" useGeneratedKeys="true" keyProperty="id">
        insert into tsbz_jyykhrw
        <trim prefix="(" suffix=")" suffixOverrides=",">
            <if test="rwmc != null">rwmc,</if>
            <if test="rwlx != null">rwlx,</if>
            <if test="rwnr != null">rwnr,</if>
            <if test="khbm != null">khbm,</if>
            <if test="khzq != null">khzq,</if>
            <if test="slyq != null">slyq,</if>
            <if test="fsbx != null">fsbx,</if>
            <if test="isdel != null">isdel,</if>
            <if test="createTime != null">create_time,</if>
            <if test="rwsm != null">rwsm,</if>
            <if test="deptId != null">dept_id,</if>
        </trim>
        <trim prefix="values (" suffix=")" suffixOverrides=",">
            <if test="rwmc != null">#{rwmc},</if>
            <if test="rwlx != null">#{rwlx},</if>
            <if test="rwnr != null">#{rwnr},</if>
            <if test="khbm != null">#{khbm},</if>
            <if test="khzq != null">#{khzq},</if>
            <if test="slyq != null">#{slyq},</if>
            <if test="fsbx != null">#{fsbx},</if>
            <if test="isdel != null">#{isdel},</if>
            <if test="createTime != null">#{createTime},</if>
            <if test="rwsm != null">#{rwsm},</if>
            <if test="deptId != null">#{deptId},</if>
        </trim>
    </insert>

    <update id="updateTsbzJyykhrw" parameterType="TsbzJyykhrw">
        update tsbz_jyykhrw
        <trim prefix="SET" suffixOverrides=",">
            <if test="rwmc != null">rwmc = #{rwmc},</if>
            <if test="rwlx != null">rwlx = #{rwlx},</if>
            <if test="rwnr != null">rwnr = #{rwnr},</if>
            <if test="khbm != null">khbm = #{khbm},</if>
            <if test="khzq != null">khzq = #{khzq},</if>
            <if test="slyq != null">slyq = #{slyq},</if>
            <if test="fsbx != null">fsbx = #{fsbx},</if>
            <if test="isdel != null">isdel = #{isdel},</if>
            <if test="createTime != null">create_time = #{createTime},</if>
            <if test="rwsm != null">rwsm = #{rwsm},</if>
            <if test="deptId != null">dept_id = #{deptId},</if>
        </trim>
        where id = #{id}
    </update>

    <delete id="deleteTsbzJyykhrwById" parameterType="Long">
        delete from tsbz_jyykhrw where id = #{id}
    </delete>

    <delete id="deleteTsbzJyykhrwByIds" parameterType="String">
        delete from tsbz_jyykhrw where id in
        <foreach item="id" collection="array" open="(" separator="," close=")">
            #{id}
        </foreach>
    </delete>

</mapper>