    <sql id="selectTsbzJxjsdsfpVo">
        -- select id, jxjsid, dsid, create_time from tsbz_jxjsdsfp
        select b.id, a.jxjsid, a.dsid, a.create_time,
b.name, b.jxbh, b.xb, b.csrq, b.email, b.phone, b.yzbm, b.zzmm, b.mz, b.prdwid, b.prdwmc, b.jdxid, b.rjxd, b.rjxk, b.rjnj, b.byyx, b.xl, b.xw, b.sfsfs, b.lqnf,
c.jdxmc as jdxmc
from  tsbz_jxjsjbxx b
left join tsbz_jdx c on b.jdxid=c.id
left join tsbz_jxjsdsfp a on a.jxjsid=b.id
    </sql>

    <select id="selectTsbzJxjsdsfpList" parameterType="TsbzJxjsdsfp" resultMap="TsbzJxjsdsfpResult">
        <include refid="selectTsbzJxjsdsfpVo"/>
        <where>  
            <if test="jxjsid != null "> and b.id = #{jxjsid}</if>
            <if test="dsid != null "> and a.dsid = #{dsid}</if>
        </where>
    </select>

    <select id="selectTsbzJxjsdsfpListExport" parameterType="TsbzJxjsdsfp" resultMap="TsbzJxjsdsfpResult">
        select b.id, a.jxjsid, a.dsid, a.create_time, e.jsxm as dsname,
        b.name as jsname, b.jxbh, b.xb, b.csrq, b.email, b.phone, b.yzbm, b.zzmm, b.mz, b.prdwid, b.prdwmc, b.jdxid, b.rjxd, b.rjxk, b.rjnj, b.byyx, b.xl, b.xw, b.sfsfs, b.lqnf,
        c.jdxmc as jdxmc
        from  tsbz_jxjsjbxx b
        left join tsbz_jdx c on b.jdxid=c.id
        left join tsbz_jxjsdsfp a on a.jxjsid=b.id
        left join tsbz_dsjbxx e on a.dsid=e.id
        <where>
            <if test="jxjsid != null "> and b.id = #{jxjsid}</if>
            <if test="dsid != null "> and a.dsid = #{dsid}</if>
        </where>
    </select>
    
    <select id="selectTsbzJxjsdsfpById" parameterType="Long" resultMap="TsbzJxjsdsfpResult">
        <include refid="selectTsbzJxjsdsfpVo"/>
        where a.id = #{id}
    </select>
        
    <insert id="insertTsbzJxjsdsfp" parameterType="TsbzJxjsdsfp" useGeneratedKeys="true" keyProperty="id">
        insert into tsbz_jxjsdsfp
        <trim prefix="(" suffix=")" suffixOverrides=",">
            <if test="jxjsid != null">jxjsid,</if>
            <if test="dsid != null">dsid,</if>
            <if test="createTime != null">create_time,</if>
         </trim>
        <trim prefix="values (" suffix=")" suffixOverrides=",">
            <if test="jxjsid != null">#{jxjsid},</if>
            <if test="dsid != null">#{dsid},</if>
            <if test="createTime != null">#{createTime},</if>
         </trim>
    </insert>

    <update id="updateTsbzJxjsdsfp" parameterType="TsbzJxjsdsfp">
        update tsbz_jxjsdsfp
        <trim prefix="SET" suffixOverrides=",">
            <if test="jxjsid != null">jxjsid = #{jxjsid},</if>
            <if test="dsid != null">dsid = #{dsid},</if>
            <if test="createTime != null">create_time = #{createTime},</if>
        </trim>
        where id = #{id}
    </update>

    <delete id="deleteTsbzJxjsdsfpById" parameterType="Long">
        delete from tsbz_jxjsdsfp where id = #{id}
    </delete>

    <delete id="deleteTsbzJxjsdsfpByIds" parameterType="String">
        delete from tsbz_jxjsdsfp where id in 
        <foreach item="id" collection="array" open="(" separator="," close=")">
            #{id}
        </foreach>
    </delete>
    
