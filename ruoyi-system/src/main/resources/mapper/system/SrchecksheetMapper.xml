<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
"http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.ruoyi.system.mapper.SrchecksheetMapper">
    
    <resultMap type="Srchecksheet" id="SrchecksheetResult">
        <result property="id"    column="id"    />
        <result property="supplier"    column="supplier"    />
        <result property="billnumber"    column="billnumber"    />
        <result property="content"    column="content"    />
        <result property="rmb"    column="rmb"    />
        <result property="paymentmethod"    column="paymentmethod"    />
        <result property="remark"    column="remark"    />
        <result property="examine"    column="examine"    />
        <result property="section"    column="section"    />
        <result property="reimbursement"    column="reimbursement"    />
        <result property="reimburser"    column="reimburser"    />
        <result property="createtime"    column="createtime"    jdbcType="TIMESTAMP"/>
        <result property="status"    column="status"    />
        <result property="updatetime"    column="updatetime"    />
        <result property="reserve1"    column="reserve1"    />
        <result property="reserve2"    column="reserve2"    />
        <result property="reserve3"    column="reserve3"    />
        <result property="reserve4"    column="reserve4"    />
    </resultMap>

    <sql id="selectSrchecksheetVo">
        select id, supplier, billnumber, content, rmb, paymentmethod, remark, examine, section, reimbursement, reimburser, createtime, status, updatetime, reserve1, reserve2, reserve3, reserve4 from srchecksheet
    </sql>

    <select id="selectSrchecksheetList" parameterType="Srchecksheet" resultMap="SrchecksheetResult">
        <include refid="selectSrchecksheetVo"/>
        <where>  
            <if test="supplier != null  and supplier != ''"> and supplier = #{supplier}</if>
            <if test="billnumber != null "> and billnumber = #{billnumber}</if>
            <if test="content != null  and content != ''"> and content = #{content}</if>
            <if test="rmb != null "> and rmb = #{rmb}</if>
            <if test="paymentmethod != null "> and paymentmethod = #{paymentmethod}</if>
            <if test="examine != null "> and examine = #{examine}</if>
            <if test="section != null "> and section = #{section}</if>
            <if test="reimbursement != null  and reimbursement != ''"> and reimbursement = #{reimbursement}</if>
            <if test="reimburser != null  and reimburser != ''"> and reimburser = #{reimburser}</if>
            <if test="createtime != null "> and createtime = #{createtime}</if>
            <if test="status != null "> and status = #{status}</if>
            <if test="updatetime != null "> and updatetime = #{updatetime}</if>
            <if test="reserve1 != null  and reserve1 != ''"> and reserve1 = #{reserve1}</if>
            <if test="reserve2 != null  and reserve2 != ''"> and reserve2 = #{reserve2}</if>
            <if test="reserve3 != null "> and reserve3 = #{reserve3}</if>
            <if test="reserve4 != null "> and reserve4 = #{reserve4}</if>
        </where>
    </select>
    
    <select id="selectSrchecksheetById" parameterType="Long" resultMap="SrchecksheetResult">
        <include refid="selectSrchecksheetVo"/>
        where id = #{id}
    </select>
        
    <insert id="insertSrchecksheet" parameterType="Srchecksheet">
        insert into srchecksheet
        <trim prefix="(" suffix=")" suffixOverrides=",">
            <if test="id != null">id,</if>
            <if test="supplier != null">supplier,</if>
            <if test="billnumber != null">billnumber,</if>
            <if test="content != null">content,</if>
            <if test="rmb != null">rmb,</if>
            <if test="paymentmethod != null">paymentmethod,</if>
            <if test="remark != null">remark,</if>
            <if test="examine != null">examine,</if>
            <if test="section != null">section,</if>
            <if test="reimbursement != null">reimbursement,</if>
            <if test="reimburser != null">reimburser,</if>
            <if test="status != null">status,</if>
            <if test="updatetime != null">updatetime,</if>
            <if test="reserve1 != null">reserve1,</if>
            <if test="reserve2 != null">reserve2,</if>
            <if test="reserve3 != null">reserve3,</if>
            <if test="reserve4 != null">reserve4,</if>
            createtime
         </trim>
        <trim prefix="values (" suffix=")" suffixOverrides=",">
            <if test="id != null">#{id},</if>
            <if test="supplier != null">#{supplier},</if>
            <if test="billnumber != null">#{billnumber},</if>
            <if test="content != null">#{content},</if>
            <if test="rmb != null">#{rmb},</if>
            <if test="paymentmethod != null">#{paymentmethod},</if>
            <if test="remark != null">#{remark},</if>
            <if test="examine != null">#{examine},</if>
            <if test="section != null">#{section},</if>
            <if test="reimbursement != null">#{reimbursement},</if>
            <if test="reimburser != null">#{reimburser},</if>
            <if test="status != null">#{status},</if>
            <if test="updatetime != null">#{updatetime},</if>
            <if test="reserve1 != null">#{reserve1},</if>
            <if test="reserve2 != null">#{reserve2},</if>
            <if test="reserve3 != null">#{reserve3},</if>
            <if test="reserve4 != null">#{reserve4},</if>
            sysdate()
         </trim>
    </insert>

    <update id="updateSrchecksheet" parameterType="Srchecksheet">
        update srchecksheet
        <trim prefix="SET" suffixOverrides=",">
            <if test="supplier != null">supplier = #{supplier},</if>
            <if test="billnumber != null">billnumber = #{billnumber},</if>
            <if test="content != null">content = #{content},</if>
            <if test="rmb != null">rmb = #{rmb},</if>
            <if test="paymentmethod != null">paymentmethod = #{paymentmethod},</if>
            <if test="remark != null">remark = #{remark},</if>
            <if test="examine != null">examine = #{examine},</if>
            <if test="section != null">section = #{section},</if>
            <if test="reimbursement != null">reimbursement = #{reimbursement},</if>
            <if test="reimburser != null">reimburser = #{reimburser},</if>
            <if test="status != null">status = #{status},</if>
            <if test="reserve1 != null">reserve1 = #{reserve1},</if>
            <if test="reserve2 != null">reserve2 = #{reserve2},</if>
            <if test="reserve3 != null">reserve3 = #{reserve3},</if>
            <if test="reserve4 != null">reserve4 = #{reserve4},</if>
            updatetime = sysdate()
        </trim>
        where id = #{id}
    </update>

    <delete id="deleteSrchecksheetById" parameterType="Long">
        delete from srchecksheet where id = #{id}
    </delete>

    <delete id="deleteSrchecksheetByIds" parameterType="String">
        delete from srchecksheet where id in 
        <foreach item="id" collection="array" open="(" separator="," close=")">
            #{id}
        </foreach>
    </delete>
</mapper>