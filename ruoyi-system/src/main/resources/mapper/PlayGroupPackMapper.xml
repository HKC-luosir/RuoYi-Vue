<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.ruoyi.system.mapper.PlayGroupPackMapper">

    <select id="selectGroupPackByPlayId" resultType="com.ruoyi.common.core.domain.entity.play.PlayGroupPack">
        select * from t_play_group_pack where play_id = #{playId} limit 1
    </select>

</mapper>