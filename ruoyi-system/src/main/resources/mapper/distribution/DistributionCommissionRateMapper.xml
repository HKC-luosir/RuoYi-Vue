<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
"http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.ruoyi.distribution.mapper.DistributionCommissionRateMapper">
    
    <resultMap type="DistributionCommissionRate" id="DistributionCommissionRateResult">
        <result property="id"    column="id"    />
        <result property="level"    column="level"    />
        <result property="rate"    column="rate"    />
        <result property="createBy"    column="create_by"    />
        <result property="createTime"    column="create_time"    />
        <result property="updateBy"    column="update_by"    />
        <result property="updateTime"    column="update_time"    />
    </resultMap>

    <sql id="selectDistributionCommissionRateVo">
        select id, level, rate, create_by, create_time, update_by, update_time from bus_distribution_commission_rate
    </sql>

    <select id="selectDistributionCommissionRateList" parameterType="DistributionCommissionRate" resultMap="DistributionCommissionRateResult">
        <include refid="selectDistributionCommissionRateVo"/>
        <where>  
            <if test="level != null "> and level = #{level}</if>
        </where>
    </select>
    
    <select id="selectDistributionCommissionRateById" parameterType="Long" resultMap="DistributionCommissionRateResult">
        <include refid="selectDistributionCommissionRateVo"/>
        where id = #{id}
    </select>
        
    <insert id="insertDistributionCommissionRate" parameterType="DistributionCommissionRate" useGeneratedKeys="true" keyProperty="id">
        insert into bus_distribution_commission_rate
        <trim prefix="(" suffix=")" suffixOverrides=",">
            <if test="level != null">level,</if>
            <if test="rate != null">rate,</if>
            <if test="createBy != null">create_by,</if>
            <if test="createTime != null">create_time,</if>
            <if test="updateBy != null">update_by,</if>
            <if test="updateTime != null">update_time,</if>
         </trim>
        <trim prefix="values (" suffix=")" suffixOverrides=",">
            <if test="level != null">#{level},</if>
            <if test="rate != null">#{rate},</if>
            <if test="createBy != null">#{createBy},</if>
            <if test="createTime != null">#{createTime},</if>
            <if test="updateBy != null">#{updateBy},</if>
            <if test="updateTime != null">#{updateTime},</if>
         </trim>
    </insert>

    <update id="updateDistributionCommissionRate" parameterType="DistributionCommissionRate">
        update bus_distribution_commission_rate
        <trim prefix="SET" suffixOverrides=",">
            <if test="level != null">level = #{level},</if>
            <if test="rate != null">rate = #{rate},</if>
            <if test="createBy != null">create_by = #{createBy},</if>
            <if test="createTime != null">create_time = #{createTime},</if>
            <if test="updateBy != null">update_by = #{updateBy},</if>
            <if test="updateTime != null">update_time = #{updateTime},</if>
        </trim>
        where id = #{id}
    </update>

    <delete id="deleteDistributionCommissionRateById" parameterType="Long">
        delete from bus_distribution_commission_rate where id = #{id}
    </delete>

    <delete id="deleteDistributionCommissionRateByIds" parameterType="String">
        delete from bus_distribution_commission_rate where id in 
        <foreach item="id" collection="array" open="(" separator="," close=")">
            #{id}
        </foreach>
    </delete>
</mapper>