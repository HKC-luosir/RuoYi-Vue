<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
"http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.ruoyi.staff.mapper.StaffPhotoMapper">
    
    <resultMap type="StaffPhoto" id="StaffPhotoResult">
        <result property="id"    column="id"    />
        <result property="userId"    column="user_id"    />
        <result property="imgUrl"    column="img_url"    />
        <result property="ifUnlock"    column="if_unlock"    />
        <result property="unlockAmount"    column="unlock_amount"    />
        <result property="state"    column="state"    />
        <result property="noPassReason"    column="no_pass_reason"    />
        <result property="sortNum"    column="sort_num"    />
        <result property="createTime"    column="create_time"    />
        <result property="updateBy"    column="update_by"    />
        <result property="updateTime"    column="update_time"    />
    </resultMap>

    <sql id="selectStaffPhotoVo">
        select id, user_id, img_url, if_unlock, unlock_amount, state, no_pass_reason, sort_num, create_time, update_by, update_time from bus_staff_photo
    </sql>

    <select id="selectStaffPhotoList" parameterType="StaffPhoto" resultMap="StaffPhotoResult">
        <include refid="selectStaffPhotoVo"/>
        <where>  
            <if test="userId != null "> and user_id = #{userId}</if>
            <if test="ifUnlock != null  and ifUnlock != ''"> and if_unlock = #{ifUnlock}</if>
            <if test="state != null  and state != ''"> and state = #{state}</if>
        </where>
    </select>
    
    <select id="selectStaffPhotoById" parameterType="Long" resultMap="StaffPhotoResult">
        <include refid="selectStaffPhotoVo"/>
        where id = #{id}
    </select>
        
    <insert id="insertStaffPhoto" parameterType="StaffPhoto" useGeneratedKeys="true" keyProperty="id">
        insert into bus_staff_photo
        <trim prefix="(" suffix=")" suffixOverrides=",">
            <if test="userId != null">user_id,</if>
            <if test="imgUrl != null">img_url,</if>
            <if test="ifUnlock != null">if_unlock,</if>
            <if test="unlockAmount != null">unlock_amount,</if>
            <if test="state != null">state,</if>
            <if test="noPassReason != null">no_pass_reason,</if>
            <if test="sortNum != null">sort_num,</if>
            <if test="createTime != null">create_time,</if>
            <if test="updateBy != null">update_by,</if>
            <if test="updateTime != null">update_time,</if>
         </trim>
        <trim prefix="values (" suffix=")" suffixOverrides=",">
            <if test="userId != null">#{userId},</if>
            <if test="imgUrl != null">#{imgUrl},</if>
            <if test="ifUnlock != null">#{ifUnlock},</if>
            <if test="unlockAmount != null">#{unlockAmount},</if>
            <if test="state != null">#{state},</if>
            <if test="noPassReason != null">#{noPassReason},</if>
            <if test="sortNum != null">#{sortNum},</if>
            <if test="createTime != null">#{createTime},</if>
            <if test="updateBy != null">#{updateBy},</if>
            <if test="updateTime != null">#{updateTime},</if>
         </trim>
    </insert>

    <update id="updateStaffPhoto" parameterType="StaffPhoto">
        update bus_staff_photo
        <trim prefix="SET" suffixOverrides=",">
            <if test="userId != null">user_id = #{userId},</if>
            <if test="imgUrl != null">img_url = #{imgUrl},</if>
            <if test="ifUnlock != null">if_unlock = #{ifUnlock},</if>
            <if test="unlockAmount != null">unlock_amount = #{unlockAmount},</if>
            <if test="state != null">state = #{state},</if>
            <if test="noPassReason != null">no_pass_reason = #{noPassReason},</if>
            <if test="sortNum != null">sort_num = #{sortNum},</if>
            <if test="createTime != null">create_time = #{createTime},</if>
            <if test="updateBy != null">update_by = #{updateBy},</if>
            <if test="updateTime != null">update_time = #{updateTime},</if>
        </trim>
        where id = #{id}
    </update>

    <delete id="deleteStaffPhotoById" parameterType="Long">
        delete from bus_staff_photo where id = #{id}
    </delete>

    <delete id="deleteStaffPhotoByIds" parameterType="String">
        delete from bus_staff_photo where id in 
        <foreach item="id" collection="array" open="(" separator="," close=")">
            #{id}
        </foreach>
    </delete>
</mapper>