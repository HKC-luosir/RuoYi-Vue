<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
"http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.ruoyi.jdgl.mapper.TsbzJdglhdglMapper">
    
    <resultMap type="TsbzJdglhdgl" id="TsbzJdglhdglResult">
        <result property="id"    column="id"    />
        <result property="jdid"    column="jdid"    />
        <result property="name"    column="name"    />
        <result property="hdtype"    column="hdtype"    />
        <result property="hdtime"    column="hdtime"    />
        <result property="hddd"    column="hddd"    />
        <result property="scpath"    column="scpath"    />
        <result property="createUserid"    column="create_userid"    />
        <result property="createTime"    column="create_time"    />
    </resultMap>

    <sql id="selectTsbzJdglhdglVo">
        select a.id, a.jdid, a.name, a.hdtype, a.hdtime, a.hddd, a.scpath, a.create_userid, a.create_time, c.zcrid as zcrid, c.jdtype as jdtype from tsbz_jdglhdgl a
        LEFT JOIN sys_user b on b.user_id=a.create_userid
        LEFT JOIN tsbz_zcrjdcj c on c.id=a.jdid
    </sql>

    <select id="selectTsbzJdglhdglList" parameterType="TsbzJdglhdgl" resultMap="TsbzJdglhdglResult">
        <include refid="selectTsbzJdglhdglVo"/>
        <where>  
            <if test="jdid != null "> and a.jdid = #{jdid}</if>
            <if test="name != null  and name != ''"> and a.name like concat('%', #{name}, '%')</if>
            <if test="hdtype != null  and hdtype != ''"> and a.hdtype = #{hdtype}</if>
            <if test="hdtime != null "> and a.hdtime = #{hdtime}</if>
            <if test="hddd != null  and hddd != ''"> and a.hddd = #{hddd}</if>
            <if test="scpath != null  and scpath != ''"> and a.scpath = #{scpath}</if>
            <if test="createUserid != null "> and a.create_userid = #{createUserid}</if>
        </where>
    </select>
    
    <select id="selectTsbzJdglhdglById" parameterType="Long" resultMap="TsbzJdglhdglResult">
        <include refid="selectTsbzJdglhdglVo"/>
        where a.id = #{id}
    </select>
        
    <insert id="insertTsbzJdglhdgl" parameterType="TsbzJdglhdgl" useGeneratedKeys="true" keyProperty="id">
        insert into tsbz_jdglhdgl
        <trim prefix="(" suffix=")" suffixOverrides=",">
            <if test="jdid != null">jdid,</if>
            <if test="name != null">name,</if>
            <if test="hdtype != null">hdtype,</if>
            <if test="hdtime != null">hdtime,</if>
            <if test="hddd != null">hddd,</if>
            <if test="scpath != null">scpath,</if>
            <if test="createUserid != null">create_userid,</if>
            <if test="createTime != null">create_time,</if>
         </trim>
        <trim prefix="values (" suffix=")" suffixOverrides=",">
            <if test="jdid != null">#{jdid},</if>
            <if test="name != null">#{name},</if>
            <if test="hdtype != null">#{hdtype},</if>
            <if test="hdtime != null">#{hdtime},</if>
            <if test="hddd != null">#{hddd},</if>
            <if test="scpath != null">#{scpath},</if>
            <if test="createUserid != null">#{createUserid},</if>
            <if test="createTime != null">#{createTime},</if>
         </trim>
    </insert>

    <update id="updateTsbzJdglhdgl" parameterType="TsbzJdglhdgl">
        update tsbz_jdglhdgl
        <trim prefix="SET" suffixOverrides=",">
            <if test="jdid != null">jdid = #{jdid},</if>
            <if test="name != null">name = #{name},</if>
            <if test="hdtype != null">hdtype = #{hdtype},</if>
            <if test="hdtime != null">hdtime = #{hdtime},</if>
            <if test="hddd != null">hddd = #{hddd},</if>
            <if test="scpath != null">scpath = #{scpath},</if>
            <if test="createUserid != null">create_userid = #{createUserid},</if>
            <if test="createTime != null">create_time = #{createTime},</if>
        </trim>
        where id = #{id}
    </update>

    <delete id="deleteTsbzJdglhdglById" parameterType="Long">
        delete from tsbz_jdglhdgl where id = #{id}
    </delete>

    <delete id="deleteTsbzJdglhdglByIds" parameterType="String">
        delete from tsbz_jdglhdgl where id in 
        <foreach item="id" collection="array" open="(" separator="," close=")">
            #{id}
        </foreach>
    </delete>
    
</mapper>