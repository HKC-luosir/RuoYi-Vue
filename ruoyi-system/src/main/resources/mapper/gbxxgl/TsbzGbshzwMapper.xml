    <sql id="selectTsbzGbshzwVo">
        select id, gbid, rzqsny, rzzzny, jb, jibie, name, createuser, createtime from tsbz_gbshzw
    </sql>

    <select id="selectTsbzGbshzwList" parameterType="TsbzGbshzw" resultMap="TsbzGbshzwResult">
        <include refid="selectTsbzGbshzwVo"/>
        <where>  
            <if test="gbid != null "> and gbid = #{gbid}</if>
            <if test="rzqsny != null "> and rzqsny = #{rzqsny}</if>
            <if test="rzzzny != null "> and rzzzny = #{rzzzny}</if>
            <if test="jb != null  and jb != ''"> and jb = #{jb}</if>
            <if test="jibie != null  and jibie != ''"> and jibie = #{jibie}</if>
            <if test="name != null  and name != ''"> and name like concat('%', #{name}, '%')</if>
            <if test="createuser != null "> and createuser = #{createuser}</if>
            <if test="createtime != null "> and createtime = #{createtime}</if>
        </where>
    </select>
    
    <select id="selectTsbzGbshzwById" parameterType="Long" resultMap="TsbzGbshzwResult">
        select a.id, a.gbid, a.rzqsny, a.rzzzny, a.jb, a.jibie, a.name, a.createuser, a.createtime,
        b.dept_id as deptid
        from tsbz_gbshzw a
        left join tsbz_gbjbqk b
        on a.gbid=b.id
        where a.id = #{id}
    </select>
        
    <insert id="insertTsbzGbshzw" parameterType="TsbzGbshzw">
