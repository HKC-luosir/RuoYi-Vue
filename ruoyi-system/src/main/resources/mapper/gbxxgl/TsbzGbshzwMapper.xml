<mapper namespace="com.ruoyi.gbxxgl.mapper.TsbzGbshzwMapper">
    
    <resultMap type="TsbzGbshzw" id="TsbzGbshzwResult">
        <result property="id"    column="id"    />
        <result property="gbid"    column="gbid"    />
        <result property="rzqsny"    column="rzqsny"    />
        <result property="rzzzny"    column="rzzzny"    />
        <result property="jb"    column="jb"    />
        <result property="jibie"    column="jibie"    />
        <result property="name"    column="name"    />
