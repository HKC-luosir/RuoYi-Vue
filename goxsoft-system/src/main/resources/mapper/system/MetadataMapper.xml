    <sql id="selectMetadataVo">
        select md.id, aggregation_level, archives_name, archives_identifier,
        fonds_name, fonds_constituting_unit_name, electronic_record_code,
        archival_code, fonds_identifier, catalogue_number, year, retention_period,
        organizational_structure_or_function, category_code, agency_file_number,
        archives_file_number, agency_item_number, archives_item_number,
        document_sequence_number, page_number, content_description, title,
        parallel_title, alternative_title, other_title_information,
        descriptor, keyword, personal_name, abstract, class_code,
        document_number, author, date, document_type, precedence, principal_receiver,
        other_receivers, security_classification, secrecy_period, formal_characteristics,
        document_aggregation_type, total_number_of_items, total_number_of_pages,
        language, manuscript_type, annotation,
        ds.signature_format_description, ds.signature_date, ds.signer, ds.signature, ds.certificate, ds.certificate_reference, ds.signature_algorithm, ds.metadata_id, ds.ele_id ,
        ea.format_information, ea.computer_file_name, ea.computer_file_size, ea.document_creating_application, ea.information_system_description, ea.physical_record_characteristics, ea.scanning_resolution, ea.scanning_color_model, ea.image_compression_scheme, ea.current_location, ea.offline_medium_identifier, ea.offline_medium_storage_location, ea.microform_identifier, ea.metadata_id,
        rm.metadata_id, rm.intellectual_property_statement, rm.authorized_agent, rm.permission_assignment, rm.control_identifier,
        mr.f1, mr.f2,mr.f3,mr.f4,mr.f5,mr.f6,mr.f7,mr.f8,mr.f9,mr.f10,mr.f11,mr.f12,mr.f13,mr.f14,mr.f15
        from metadata md
        left join electronic_attributes ea on md.id=ea.metadata_id left join digital_signature ds on md.id=ds.metadata_id left join rights_management rm on md.id=rm.metadata_id left join metadata_reserve mr on md.id=mr.id
    </sql>

