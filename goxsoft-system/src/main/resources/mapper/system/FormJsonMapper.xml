<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
"http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.gox.system.mapper.FormJsonMapper">
    
    <resultMap type="com.gox.system.domain.FormJson" id="FormJsonResult">
        <result property="id"    column="id"    />
        <result property="createTime"    column="create_time"    />
        <result property="updateTime"    column="update_time"    />
        <result property="createBy"    column="create_by"    />
        <result property="updateBy"    column="update_by"    />
        <result property="remark"    column="remark"    />
        <result property="formName"    column="form_name"    />
        <result property="formData"    column="form_data"    />
        <result property="delFlag"    column="del_flag"    />
    </resultMap>

    <sql id="selectFormJsonVo">
        select id, create_time, update_time, create_by, update_by, remark, form_name, form_data, del_flag from form_json
    </sql>

    <select id="selectFormJsonList" parameterType="com.gox.system.domain.FormJson" resultMap="FormJsonResult">
        <include refid="selectFormJsonVo"/>
        <where>  
            <if test="formName != null  and formName != ''"> and form_name like concat('%', #{formName}, '%')</if>
            <if test="formData != null  and formData != ''"> and form_data = #{formData}</if>
        </where>
    </select>
    
    <select id="selectFormJsonById" parameterType="Long" resultMap="FormJsonResult">
        <include refid="selectFormJsonVo"/>
        where id = #{id}
    </select>
        
    <insert id="insertFormJson" parameterType="com.gox.system.domain.FormJson">
        insert into form_json
        <trim prefix="(" suffix=")" suffixOverrides=",">
            <if test="id != null">id,</if>
            <if test="createTime != null">create_time,</if>
            <if test="updateTime != null">update_time,</if>
            <if test="createBy != null and createBy != ''">create_by,</if>
            <if test="updateBy != null and updateBy != ''">update_by,</if>
            <if test="remark != null and remark != ''">remark,</if>
            <if test="formName != null and formName != ''">form_name,</if>
            <if test="formData != null and formData != ''">form_data,</if>
            <if test="delFlag != null and delFlag != ''">del_flag,</if>
         </trim>
        <trim prefix="values (" suffix=")" suffixOverrides=",">
            <if test="id != null">#{id},</if>
            <if test="createTime != null">#{createTime},</if>
            <if test="updateTime != null">#{updateTime},</if>
            <if test="createBy != null and createBy != ''">#{createBy},</if>
            <if test="updateBy != null and updateBy != ''">#{updateBy},</if>
            <if test="remark != null and remark != ''">#{remark},</if>
            <if test="formName != null and formName != ''">#{formName},</if>
            <if test="formData != null and formData != ''">#{formData},</if>
            <if test="delFlag != null and delFlag != ''">#{delFlag},</if>
         </trim>
    </insert>

    <update id="updateFormJson" parameterType="com.gox.system.domain.FormJson">
        update form_json
        <trim prefix="SET" suffixOverrides=",">
            <if test="createTime != null">create_time = #{createTime},</if>
            <if test="updateTime != null">update_time = #{updateTime},</if>
            <if test="createBy != null and createBy != ''">create_by = #{createBy},</if>
            <if test="updateBy != null and updateBy != ''">update_by = #{updateBy},</if>
            <if test="remark != null and remark != ''">remark = #{remark},</if>
            <if test="formName != null and formName != ''">form_name = #{formName},</if>
            <if test="formData != null and formData != ''">form_data = #{formData},</if>
            <if test="delFlag != null and delFlag != ''">del_flag = #{delFlag},</if>
        </trim>
        where id = #{id}
    </update>

    <delete id="deleteFormJsonById" parameterType="Long">
        delete from form_json where id = #{id}
    </delete>

    <delete id="deleteFormJsonByIds" parameterType="String">
        delete from form_json where id in 
        <foreach item="id" collection="array" open="(" separator="," close=")">
            #{id}
        </foreach>
    </delete>
    
</mapper>